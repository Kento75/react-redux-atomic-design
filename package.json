  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack.dev.config",
    "start-windows": "set NODE_ENV=development&& webpack-dev-server --mode development --config webpack.dev.config",
    "build": "NODE_ENV=production webpack --mode production --config webpack.prod.config.js --colors",
    "build-windows": "set NODE_ENV=production  && webpack --config webpack.prod.config.js --mode production --colors",
    "build-staging": "NODE_ENV=staging webpack --mode production --config webpack.prod.config.js --colors",
    "lintThis": "eslint src --ext .js,.jsx --fix",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006 -s ./assets",
    "storybook:visual": "start-storybook -p 9001 -s ./assets",
    "spec:visual": "backstop reference",
    "test:visual": "backstop test",
    "approve:visual": "backstop approve",
    "report:visual": "backstop openReport",
    "test": "cross-env STORY_SHOTS=1 jest",
    "svg": "svg-sprite-generate -d ./assets/icons -o ./assets/icons.svg"
  },
  "pre-commit": [
    "lintThis"
  ],
